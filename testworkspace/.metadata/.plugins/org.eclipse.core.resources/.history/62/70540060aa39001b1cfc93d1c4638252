package adventofcode;

public class Field {
	String rohdaten;
	String name;
	String value;
	
	public Field(String rohdaten) {
		this.name = rohdaten.substring(0, 3);
		this.value = rohdaten.substring(4,rohdaten.length());
		this.rohdaten = rohdaten;
	}
	
	public String getName() {
		return name;
	}
	
	public boolean istValide() {
		return true;
	}
}
